package pixi.geom;

/**
 * Size object, contains width and height.
 * 
 * @see https://pixijs.download/dev/docs/PIXI.html#.ISize
 */
typedef ISize =
{
    /**
     * Width component.
     */
    public var width:Float;

    /**
     * Height component.
     */
    public var height:Float;
}